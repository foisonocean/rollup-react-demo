{
  "name": "rollup-react-demo",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/foisonocean/rollup-react-demo.git",
  "author": "foisonocean <foisonocean@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "run-s clean watch",
    "watch": "run-p build:html rollup:watch browser-refresh",
    "build": "run-s clean build:files clean:cache",
    "clean": "rimraf .typescript-compile-cache build",
    "build:files": "run-p build:rollup build:html",
    "build:rollup": "cross-env NODE_ENV=production rollup -c",
    "build:html": "cpx ./index.html build",
    "rollup:watch": "rollup -c --watch",
    "browser-refresh": "browser-sync start --server 'build' --files 'build' --index 'index.html' --browser 'Google Chrome Canary'",
    "clean:build": "rimraf build",
    "clean:cache": "rimraf .typescript-compile-cache"
  },
  "devDependencies": {
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "browser-sync": "^2.18.8",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "npm-run-all": "^4.0.2",
    "resolve": "^1.3.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^2.1.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-typescript2": "^0.4.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "tslib": "^1.6.0",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "browserlist": [
    ">0.1%",
    "not ie <9"
  ]
}
